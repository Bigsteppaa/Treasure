      "source": [
        "from sklearn.ensemble import RandomForestClassifier\n",
        "\n",
        "# Create a Random Forest Classifier model for the Wine dataset\n",
        "# Using similar parameters (n_estimators=100, random_state=42) as the Iris model\n",
        "model_wine = RandomForestClassifier(n_estimators=100, random_state=42)\n",
        "\n",
        "# Train the model using the Wine training data\n",
        "model_wine.fit(X_train_wine, y_train_wine)\n",
        "\n",
        "print(\"Random Forest Classifier model trained successfully on the Wine dataset.\")"
      ],
